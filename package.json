    "highlight.js": "^9.13.1",
    "izitoast": "^1.4.0",
    "katex": "^0.10.0",
    "keccak": "^2.0.0",
    "lodash": "^4.17.11",
    "material-design-icons-iconfont": "^4.0.2",
    "monaco-editor": "^0.15.6",
