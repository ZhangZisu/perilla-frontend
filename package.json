    "@vue/cli-service": "^3.2.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^3.1.1",
    "babel-eslint": "^10.0.1",
    "electron": "^4.0.1",
    "eslint": "^5.12.0",
    "eslint-plugin-vue": "^5.1.0",
    "lint-staged": "^9.4.1",
    "monaco-editor-webpack-plugin": "^1.7.0",
